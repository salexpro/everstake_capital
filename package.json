{
  "name": "gatsby-starter-minimal",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "salexpro <dev@salex.pro>",
  "dependencies": {
    "@fontsource/ibm-plex-mono": "4.5.7",
    "@fontsource/ibm-plex-sans": "4.5.7",
    "@formspree/react": "2.2.4",
    "@gatsbyjs/reach-router": "1.3.7",
    "@percy/cli": "1.0.8",
    "@reach/router": "1.3.4",
    "@svgr/webpack": "6.2.1",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "gatsby": "4.9.1",
    "gatsby-plugin-image": "2.9.0",
    "gatsby-plugin-minify-classnames": "4.1.2",
    "gatsby-plugin-offline": "5.9.0",
    "gatsby-plugin-purgecss": "6.1.2",
    "gatsby-plugin-react-helmet": "5.9.0",
    "gatsby-plugin-sass": "5.9.0",
    "gatsby-plugin-sharp": "4.9.0",
    "gatsby-plugin-svgr-svgo": "2.0.0",
    "gatsby-source-filesystem": "4.9.0",
    "gatsby-transformer-json": "4.9.0",
    "gatsby-transformer-sharp": "4.9.0",
    "gsap": "3.9.1",
    "javascript-terminal": "1.1.1",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-bootstrap": "2.2.3",
    "react-dom": "17.0.2",
    "react-fast-marquee": "1.3.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.29.0",
    "react-terminal-component": "1.5.0",
    "sass": "1.50.0",
    "styled-components": "5.3.5",
    "svg-parser": "2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -o",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,mdx,md,css,scss}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install",
    "cz": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
