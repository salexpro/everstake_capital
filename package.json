{
  "name": "gatsby-starter-minimal",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "salexpro <dev@salex.pro>",
  "dependencies": {
    "@fontsource/ibm-plex-mono": "4.5.13",
    "@fontsource/ibm-plex-sans": "4.5.13",
    "@formspree/react": "2.4.1",
    "@gatsbyjs/reach-router": "1.3.9",
    "@percy/cli": "1.18.0",
    "@reach/router": "1.3.4",
    "@svgr/webpack": "6.5.1",
    "bootstrap": "5.2.3",
    "classnames": "2.3.2",
    "gatsby": "4.25.4",
    "gatsby-plugin-image": "2.25.0",
    "gatsby-plugin-minify-classnames": "4.1.3",
    "gatsby-plugin-offline": "5.25.0",
    "gatsby-plugin-purgecss": "6.2.1",
    "gatsby-plugin-react-helmet": "5.25.0",
    "gatsby-plugin-sass": "5.25.0",
    "gatsby-plugin-sharp": "4.25.0",
    "gatsby-plugin-svgr-svgo": "2.0.0",
    "gatsby-source-filesystem": "4.25.0",
    "gatsby-transformer-json": "4.25.0",
    "gatsby-transformer-sharp": "4.25.0",
    "gsap": "3.11.4",
    "javascript-terminal": "1.1.1",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "2.7.2",
    "react-dom": "18.2.0",
    "react-fast-marquee": "1.3.5",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.43.1",
    "react-terminal-component": "1.5.0",
    "sass": "1.58.0",
    "styled-components": "5.3.6",
    "svg-parser": "2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-conventional": "17.4.3",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "12.5.0",
    "prettier": "2.8.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -o",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,mdx,md,css,scss}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install",
    "cz": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
